    <validationRules>
        <fullName>CandidateOnContact_CandidateIndustry</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(RecordTypeId = &quot;012b0000000Tq7T&quot;),
ISPICKVAL(Candidate_Status__c, &quot;Active&quot;),
ISBLANK(Employer_Types__c)
)</errorConditionFormula>
        <errorDisplayField>Employer_Types__c</errorDisplayField>
        <errorMessage>Please fill in the Candidate Industry for this Contact (because you have selected &quot;Active&quot; for the candidate status).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CandidateOnContact_CustomerSectors</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(RecordTypeId = &quot;012b0000000Tq7T&quot;),
ISPICKVAL(Candidate_Status__c, &quot;Active&quot;),
ISBLANK(Customer_Sectors__c)
)</errorConditionFormula>
        <errorDisplayField>Customer_Sectors__c</errorDisplayField>
        <errorMessage>Please fill in the Customer Sectors for this Contact (because you have selected &quot;Active&quot; for the candidate status).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CandidateOnContact_EmploymentPref</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(RecordTypeId = &quot;012b0000000Tq7T&quot;),
ISPICKVAL(Candidate_Status__c, &quot;Active&quot;),
ISPICKVAL(Employment_Preference__c, &quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>Employment_Preference__c</errorDisplayField>
        <errorMessage>Please fill in the Employment Preference for this Contact (because you have selected &quot;Active&quot; for the candidate status).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CandidateOnContact_Source</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(RecordTypeId = &quot;012b0000000Tq7T&quot;),
ISPICKVAL(Candidate_Status__c, &quot;Active&quot;),
ISPICKVAL(Original_Candidate_Source__c,&quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>Original_Candidate_Source__c</errorDisplayField>
        <errorMessage>Please fill in the Original Candidate Source for this Contact (because you have selected &quot;Active&quot; for the candidate status).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CandidateOnContact_Technology</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(RecordTypeId = &quot;012b0000000Tq7T&quot;),
ISPICKVAL(Candidate_Status__c, &quot;Active&quot;),
ISBLANK(Technology_Sector__c)
)</errorConditionFormula>
        <errorDisplayField>Technology_Sector__c</errorDisplayField>
        <errorMessage>Please fill in the Technology Sector for this Contact (because you have selected &quot;Active&quot; for the candidate status).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Country_Candidate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
RecordTypeId=&quot;012b0000000Tq7T&quot;,
ISPICKVAL(OtherCountryCode, &quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>OtherCountryCode</errorDisplayField>
        <errorMessage>Please enter the Home Country for this Candidate.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Country_Contact</fullName>
        <active>true</active>
        <description>Checks there is a Country for Contacts and Contact:Admins.</description>
        <errorConditionFormula>AND(
OR(RecordTypeId=&quot;012b0000000Tq7O&quot;,RecordTypeId=&quot;012b0000000UMXs&quot;)
,
ISPICKVAL(MailingCountryCode, &quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>MailingCountryCode</errorDisplayField>
        <errorMessage>Please enter the Business Country for this Contact.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>First_Name</fullName>
        <active>true</active>
        <errorConditionFormula>FirstName = &quot;&quot;</errorConditionFormula>
        <errorDisplayField>FirstName</errorDisplayField>
        <errorMessage>Please enter the persons&apos; first name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MarketingEmail_Check_Exists</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 

AND(
ISPICKVAL(Preferred_Email_Address__c , &quot;Business Email&quot;),
ISBLANK(Email_Business__c)
),
AND(
ISPICKVAL(Preferred_Email_Address__c , &quot;Personal Email&quot;),
ISBLANK(Email_Personal__c)
),
AND(
ISPICKVAL(Preferred_Email_Address__c , &quot;Other Email&quot;),
ISBLANK(Email_Other__c)
)
)</errorConditionFormula>
        <errorDisplayField>Preferred_Email_Address__c</errorDisplayField>
        <errorMessage>Please only select an email address that exists.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MarketingEmail_NotMadeBlank</fullName>
        <active>true</active>
        <description>This is check to ensure the email is not &apos;blanked&apos; - because the email address is Campaign Monitor&apos;s unique identifier</description>
        <errorConditionFormula>AND( 
NOT (PRIORVALUE( Email )= &quot;&quot;), 
ISBLANK( Email )  
)</errorConditionFormula>
        <errorDisplayField>Email</errorDisplayField>
        <errorMessage>Once an email address is added, this value can not be made blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MarketingEmail_PickOne</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
NOT(ISBLANK(Email_Business__c)),
NOT(ISBLANK(Email_Personal__c)),
NOT(ISBLANK(Email_Other__c))
),
ISPICKVAL(Preferred_Email_Address__c,&quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>Preferred_Email_Address__c</errorDisplayField>
        <errorMessage>Please select which email address is preferred for marketing materials.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Not_Until_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
ISPICKVAL(Sales_Campaign__c , &quot;None Until...&quot;),
ISBLANK(No_Sales_Emails_Until__c)
)</errorConditionFormula>
        <errorDisplayField>No_Sales_Emails_Until__c</errorDisplayField>
        <errorMessage>Please enter the date when we can start sending sales emails again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Not_Until_Date_Not_Blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
NOT (ISPICKVAL(Sales_Campaign__c , &quot;None Until...&quot;)),
NOT (ISBLANK(No_Sales_Emails_Until__c))
)</errorConditionFormula>
        <errorDisplayField>No_Sales_Emails_Until__c</errorDisplayField>
        <errorMessage>No need to enter a date here unless you select &quot;None Until...&quot; in the Sales Campaign field.</errorMessage>
    </validationRules>

    <validationRules>
        <fullName>City_and_Country_Check</fullName>
        <active>true</active>
        <description>Checks that a City and Country has been added for Company HQs, Acquired Companies, Investors and Suppliers, not Employer or Branch accounts.</description>
        <errorConditionFormula>AND(
 OR(
  RecordType.Id = &quot;012b0000000TpLO&quot;,
  RecordType.Id = &quot;012b0000000TpLs&quot;,
  RecordType.Id = &quot;012b0000000Tyc8&quot;,
  RecordType.Id = &quot;012b0000000UMgq&quot;
 ),
 OR(
  BillingCity = NULL,
  ISPICKVAL(BillingCountryCode,&quot;&quot;)
 )
)</errorConditionFormula>
        <errorMessage>Please enter the City and Country.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Nearest_Major_City_Check</fullName>
        <active>false</active>

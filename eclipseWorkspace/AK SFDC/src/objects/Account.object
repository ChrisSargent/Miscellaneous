  RecordType.Id = &quot;012b0000000TpLs&quot;,
  RecordType.Id = &quot;012b0000000Tyc8&quot;,
  RecordType.Id = &quot;012b0000000UMgq&quot;
 ),
 AND(
  ISPICKVAL(BillingCountryCode,&quot;US&quot;),
  ISPICKVAL(Nearest_Major_City__c, &quot;&quot;)
 )
)</errorConditionFormula>
